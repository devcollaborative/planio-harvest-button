{
  "manifest_version": 2,
  "name": "Planio Harvest Button",
  "version": "1",
  "description": "Adds a Harvest time tracking button to Planio issue pages.",
  "icons": {
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.plan.io/issues/*"],
      "js": ["planio-harvest.js"],
      "run_at": "document_start"
    }
  ]
}
